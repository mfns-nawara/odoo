<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="res_partner_form_l10n_se" model="ir.ui.view">
        <field name="name">res.partner.form.l10n_se</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='vat']" position="after">
                    <field name="orgnumber" attrs="{'invisible': [('is_company','=', False)]}" readonly="1"/>
            </xpath>
        </field>
    </record>
</odoo>
